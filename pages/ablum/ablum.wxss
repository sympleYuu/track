/* pages/ablum/ablum.wxss */
.ablumbox{
  width: 702rpx;
  margin: 0 auto;
  position: relative;
  margin-bottom: 32rpx;
}
.ablumbox .tit{
  font-size: 46rpx;
  position: absolute;
  width: 100%;
  text-align: center;
  color: #fff;
  top: 120rpx;
}
.ablumbox image{
  width: 100%;
  height: 398rpx;
  background: #743c60;
  border-radius: 7rpx;
}
.numtit{
  position: absolute;
  top: 200rpx;
  left: 0;
  color: #fff;
  font-size: 28rpx;
  width: 100%;
  text-align: center;
  letter-spacing: 5rpx;
}