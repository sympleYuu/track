/* pages/videolist/videolist.wxss */
.videobox{
  width: 702rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
}
.videobox video{
  width: 100%;
  height: 398rpx;
}
.vedall{
  width: 702rpx;
  height: 398rpx;
  margin: 0 auto;
  border-radius: 8rpx;
  position: relative;
}
.videobox .cover{
  width: 100%;
  height: 398rpx;
  background: #eee;
  border-radius: 8rpx;
}
.playbtn{
  width: 66rpx;
  height: 66rpx;
  position: absolute;
  left: 50%;
  margin-left: -33rpx;
  margin-top: -33rpx;
  top: 50%;
}
.vedtit{
  color:#1f1607;
  font-size: 28rpx;
  line-height: 1.5;
  margin-top: 18rpx;
}
.photobox{
  width: 700rpx;
  margin: 0 auto;
}
.photobox .title{
  text-align: center;
  margin-bottom: 50rpx;
  font-size: 36rpx;
  color: #333;
}